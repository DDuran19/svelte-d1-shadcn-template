		"vite": "^6.1.0",
		"wrangler": "^3.109.1",
		"zod": "^3.24.2"
	},
	"dependencies": {
		"@hono/swagger-ui": "^0.5.0",
		"@hono/zod-validator": "^0.4.3",
		"@tailwindcss/vite": "^4.0.6",
		"@tanstack/table-core": "^8.21.2",
		"bcryptjs": "^3.0.1",
		"dayjs": "^1.11.13",
		"dayjs-plugin-utc": "^0.1.2",
		"dexie": "^4.0.11",
		"drizzle-orm": "^0.39.3",
		"elea-di": "^1.0.9-charlie",
		"hono": "^4.7.1",
		"nanoid": "^5.1.0"
	}
